
{
    "name": "Learn That",
    "manifest_version": 3,
    "version": "1.1",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "offscreen",
      "activeTab",
      "storage",
      "scripting"

    ],
    "action": {
        "default_popup": "popup.html"
      },
      "content_scripts" : [
        {
          "matches": ["<all_urls>"],
          "js": ["contentscript.js"]
        }
  
  
      ]
  }
  