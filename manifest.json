{
  "name": "Learn That",
  "manifest_version": 3,
  "version": "1.1",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "identity",
    "offscreen"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://identitytoolkit.googleapis.com/*"
    
    
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self';"
  }
  
}